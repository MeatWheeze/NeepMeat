{
  "multipart": [
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "neepmeat:block/check_valve"
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "neepmeat:block/check_valve",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "neepmeat:block/check_valve",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "neepmeat:block/check_valve",
        "y": -90
      }
    },
    {
      "when": {
        "facing": "up"
      },
      "apply": {
        "model": "neepmeat:block/check_valve",
        "x": -90
      }
    },
    {
      "when": {
        "facing": "down"
      },
      "apply": {
        "model": "neepmeat:block/check_valve",
        "x": 90
      }
    },
    {
      "apply": {
        "model": "neepmeat:block/rusty_pipe/pipe_test"
      }
    }
  ]
}
